pipeline
{
agent any
stages
      {
            Stage ('git clone')
            { steps
             { echo'downloading code'}
            }
            
            Stage ('code compile')
            { steps
             { echo 'code is compliling'}
            }
            
            Stage ('code execution')
            { steps
             { echo 'code is executing'}
            }
            
            Stage ('code build')
            { steps
             { echo 'code is building now'}
            }
      }
}
