pipeline
{
agent any
stages
      {
            Stage ('git clone')
            { steps
                   { sh 'downloading code'}
            }
            
            Stage ('code compile')
            { steps
                   { sh 'code is compliling'}
            }
            
            Stage ('code execution')
            { steps
                   { sh 'code is executing'}
            }
            
            Stage ('code build')
            { steps
                   { sh 'code is building'}
            }
      }
}
